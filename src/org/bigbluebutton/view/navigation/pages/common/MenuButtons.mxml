<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:ui="org.bigbluebutton.view.ui.*"
		 width="100%"
		 >
	<fx:Script>
		<![CDATA[	
			import org.bigbluebutton.view.navigation.pages.PagesENUM;		
					
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:layout>
		<s:HorizontalLayout gap="0" horizontalAlign="center" paddingBottom="0" paddingLeft="0" paddingRight="0" paddingTop="0"/>
	</s:layout>
	<ui:NavigationButton id="presentationBtn" width="100%" navigateTo="{[PagesENUM.PRESENTATION]}" styleName="presentationBtnStyle" height="{getStyle('height')}"/>
	<ui:NavigationButton id="videochatBtn" navigateTo="{[PagesENUM.VIDEO_CHAT]}" width="100%" styleName="videochatBtnStyle" height="{getStyle('height')}"/>
	<ui:NavigationButton id="chatBtn" navigateTo="{[PagesENUM.CHATROOMS,PagesENUM.CHAT,PagesENUM.SELECT_PARTICIPANT]}" width="100%" styleName="chatBtnStyle" height="{getStyle('height')}"/>
	<ui:NavigationButton id="participantsBtn" navigateTo="{[PagesENUM.PARTICIPANTS,PagesENUM.USER_DETAIS]}" width="100%" styleName="participantsBtnStyle" height="{getStyle('height')}"/>
</s:Group>
